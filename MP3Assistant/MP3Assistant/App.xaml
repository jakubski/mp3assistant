<Application x:Class="MP3Assistant.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:MP3Assistant"
             StartupUri="Views/MainWindow.xaml">
    <Application.Resources>
        <local:ApplicationPageToPageConverter x:Key="PageConverter" />
        <local:StringArrayToStringConverter x:Key="MultipleArtistsConverter" />
        <local:BoolToCharConverter x:Key="VisibilityToIconConverter"/>
    </Application.Resources>
</Application>
